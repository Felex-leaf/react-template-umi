{
  "private": true,
  "author": "wb_yefei02 <wb_yefei02@meituan.com>",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "lint": "npm run lint:script && npm run lint:style && npm run lint:ts",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:style": "stylelint **/*.less",
    "lint:ts": "tsc --noEmit --project tsconfig.json",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "umi": "^4.0.61"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,json,css,less,scss,html,md}": [
      "prettier --write -l",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "*.less": "stylelint"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.1",
    "stylelint-config": "^0.1.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^4.1.2"
  }
}
